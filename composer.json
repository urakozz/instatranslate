{
	"name": "urakozz/instatranslate",
	"description": "Instatranslate",
	"keywords": ["framework"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "~5.0.0",
		"react/react":"~0.4.2",
		"predis/predis":"~1.0",
		"jms/serializer":"*",
		"guzzlehttp/guzzle":"~5.0",
		"doctrine/common": "~2.5"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1",
	    "filp/whoops": "~1.0",
		"barryvdh/laravel-ide-helper": "~2.0",
		"codeclimate/php-test-reporter": "dev-master"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"_post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"_post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
